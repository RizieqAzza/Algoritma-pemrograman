#include <iostream>
using namespace std;

int main() {
    cout << "Bilangan" << endl;
    
    cout << "Ganjil : ";
    for (int i = 1; i < 36; i += 2) {
        cout << i << " ";
    }

    cout << endl << "Genap : ";
    for (int i = 2; i < 35; i += 2) {
        cout << i << " ";
    }

    cout << endl;
    return 0;
}
